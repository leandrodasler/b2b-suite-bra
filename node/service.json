{
  "memory": 1024,
  "ttl": 1440,
  "timeout": 60,
  "cpu": {
    "type": "shared",
    "value": 5,
    "scalingPercentage": 20
  },
  "minReplicas": 8,
  "maxReplicas": 80,
  "workers": 1,
  "routes": {
    "monthlyOrders": {
      "path": "/_v/private/b2b-suite-bra/monthly-orders",
      "public": true,
      "policies": [
        {
          "effect": "allow",
          "actions": ["get", "options"]
        }
      ]
    },
    "orders": {
      "path": "/_v/private/b2b-suite-bra/orders",
      "public": true,
      "policies": [
        {
          "effect": "allow",
          "actions": ["get", "options"]
        }
      ]
    },
    "order": {
      "path": "/_v/private/b2b-suite-bra/orders/:orderId",
      "public": true,
      "policies": [
        {
          "effect": "allow",
          "actions": ["get", "options"]
        }
      ]
    },
    "status": {
      "path": "/_v/status/:code",
      "public": true
    }
  }
}
